meta {
  name: Check Email (Invalid)
  type: http
  seq: 5
}

get {
  url: {{sa_baseUrl}}/artists/apply/check-email?email=not-an-email
  body: none
  auth: none
}

assert {
  res.status: eq 400
  res.body.error.code: eq VALIDATION_ERROR
}

tests {
  test("should return validation error for invalid email", function() {
    const body = res.getBody();
    expect(res.getStatus()).to.equal(400);
    expect(body.error.code).to.equal("VALIDATION_ERROR");
  });
}
