meta {
  name: Check Email
  type: http
  seq: 4
}

get {
  url: {{sa_baseUrl}}/artists/apply/check-email?email=new-applicant@example.com
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.exists: eq false
}

tests {
  test("should return exists: false for new email", function() {
    const body = res.getBody();
    expect(res.getStatus()).to.equal(200);
    expect(body.exists).to.equal(false);
  });
}
