meta {
  name: Protected Route (No Token)
  type: http
  seq: 1
}

get {
  url: {{sa_baseUrl}}/me/dashboard
  body: none
  auth: none
}

assert {
  res.status: eq 401
  res.body.error.code: eq UNAUTHORIZED
}

tests {
  test("should return 401 when no auth token is provided", function() {
    const body = res.getBody();
    expect(body.error.code).to.equal("UNAUTHORIZED");
    expect(body.error.message).to.equal("Authentication required");
  });
}
