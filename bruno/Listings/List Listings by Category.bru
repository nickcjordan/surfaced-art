meta {
  name: List Listings by Category
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/listings?category={{category}}&status=available
  body: none
  auth: none
}

assert {
  res.status: eq 200
}

tests {
  test("all listings should match the category filter", function() {
    const listings = res.getBody().data;
    const category = bru.getEnvVar("category");
    listings.forEach(function(listing) {
      expect(listing.category).to.equal(category);
    });
  });
}
