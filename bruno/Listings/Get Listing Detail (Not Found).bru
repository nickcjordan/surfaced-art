meta {
  name: Get Listing Detail (Not Found)
  type: http
  seq: 5
}

get {
  url: {{sa_baseUrl}}/listings/00000000-0000-0000-0000-000000000000
  body: none
  auth: none
}

assert {
  res.status: eq 404
  res.body.error.code: eq NOT_FOUND
}

tests {
  test("should return 404 for non-existent listing", function() {
    const body = res.getBody();
    expect(body.error.code).to.equal("NOT_FOUND");
    expect(body.error.message).to.equal("Listing not found");
  });
}
